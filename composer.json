{
    "name": "marc-mabe/php-enum",
    "description": "Simple and fast implementation of enumerations with native PHP>=5.6",
    "type": "library",
    "keywords": [
        "enum", "enumeration", "enumerator",
        "enumset", "enum-set", "set",
        "enummap", "enum-map", "map",
        "type", "typehint", "type-hint"
    ],
    "homepage": "https://github.com/marc-mabe/php-enum",
    "authors": [{
        "name":     "Marc Bennewitz",
        "email":    "dev@mabe.berlin",
        "homepage": "http://mabe.berlin/",
        "role":     "Lead"
    }],
    "license": "BSD-3-Clause",
    "require": {
        "php":            ">=5.6",
        "ext-reflection": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "phpbench/phpbench": "@dev",

        "lstrojny/functional-php": "HHVM: variadic params with type constraints are not supported in non-Hack",
        "lstrojny/functional-php": "1.2.* || 1.0.*"
    },
    "autoload": {
        "psr-4": {
            "MabeEnum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MabeEnumTest\\":  "tests/MabeEnumTest/",
            "MabeEnumBench\\": "bench/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev",
            "dev-4.x":    "4.0-dev",
            "dev-2.x":    "2.3-dev",
            "dev-1.x":    "1.3-dev"
        }
    }
}
